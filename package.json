    "@aws-sdk/s3-request-presigner": "^3.787.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/form-data": "^2.2.1",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.17.30",
